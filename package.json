  "author": "CUP Contributors <cup@computeruseprotocol.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/computeruseprotocol/typescript-sdk"
  },
  "homepage": "https://computeruseprotocol.com",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "zod": "^3.24.0",
    "screenshot-desktop": "^1.15.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
