{
  "name": "computeruseprotocol",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Computer Use Protocol â€” universal cross-platform UI accessibility",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "cup": "./dist/cli.js",
    "cup-mcp": "./dist/mcp/cli.js"
  },
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "accessibility",
    "ui-automation",
    "ai-agents",
    "computer-use",
    "aria",
    "cup"
